

{
  "env": {
    "browser": true,
    "es6": true,
    "jest": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "extends": ["airbnb-base"],
  "rules": {
    "no-shadow": "off",
    "no-param-reassign": "off",
    "eol-last": "off",
    "no-unused-expressions":"off",
    "no-useless-constructor":"off",
    "class-methods-use-this":"off",
    "no-restricted-globals":"off",
    "arrow-parens": "off",
    "import/no-extraneous-dependencies": ["error", {"devDependencies": true}]
  },
  "ignorePatterns": [
    "dist/",
    "build/"
  ]
  
}